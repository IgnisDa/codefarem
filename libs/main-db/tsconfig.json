{
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "noUnusedLocals": false,
    "emitDeclarationOnly": false,
    "resolveJsonModule": true,
    "module": "Node16",
    "types": [
      "jest",
      "node"
    ],
    "paths": {
      ":faker": [
        "../faker/src/index.ts"
      ],
      ":generated/*": [
        "../generated/src/*"
      ]
    },
    "outDir": "../../.moon/cache/types/libs/main-db"
  },
  "include": [
    "jest.config.ts",
    "**/*.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.d.ts",
    "tsconfig.json"
  ],
  "exclude": [],
  "references": [
    {
      "path": "../faker"
    },
    {
      "path": "../generated"
    }
  ]
}
